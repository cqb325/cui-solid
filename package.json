{
  "name": "cui-solid",
  "version": "1.0.1",
  "description": "An UI component library and front-end solution based on solidjs",
  "type": "module",
  "author": "cqb325",
  "homepage": "https://cqb325.gitee.io/cui-solid-doc",
  "main": "dist/cui.min.esm.js",
  "keywords": [
    "cui-solid",
    "solid-js",
    "solid",
    "web",
    "library",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/cqb325/cui-solid"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "npm run build:lib && npm run build:style",
    "build:lib": "vite build",
    "build:types": "tsc",
    "build:style": "gulp --gulpfile build/build.style.ts",
    "build:mjs": "tsc --project tsconfig.src.json --outDir ./dist-jsx --module ESNext",
    "build:doc": "vite build --config build/vite.doc.config.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "async-validator": "^4.2.5",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-solid": "^0.13.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "highlight.js": "^11.8.0",
    "jsdom": "20.0.2",
    "less": "^4.1.3",
    "solid-js": "^1.8.18",
    "solid-testing-library": "0.5.0",
    "solid-vue-router": "^1.0.15",
    "typescript": "*",
    "vite": "^4.3.9",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "0.25.1"
  },
  "dependencies": {
    "countup.js": "^2.6.2",
    "dayjs": "^1.11.7",
    "tinycolor2": "^1.6.0"
  }
}